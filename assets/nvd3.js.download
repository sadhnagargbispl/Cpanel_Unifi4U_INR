        nvd3HorizontalBar2('#nvd3-horizontal-bar-2 svg', colors);
        nvd3Line1('#nvd3-line-1 svg', colors);
        nvd3Line2('#nvd3-line-2 svg', colors);
        nvd3Scatter1('#nvd3-scatter svg', colors);
    });
})();
