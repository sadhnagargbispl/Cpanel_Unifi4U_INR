(function() {
    $(function() {
        if (!element_exists('#charts-nvd3')) {
            return false;
        }
        var colors = bootstrap_colors();
        nvd3Bar1('#nvd3-bar-1 svg', colors);
        nvd3Bar2('#nvd3-bar-2 svg', colors);
        nvd3Pie1('#nvd3-pie svg', colors);
        nvd3Donut1('#nvd3-donut svg', colors);
